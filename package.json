{
  "name": "pikertattoos",
  "version": "0.14.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "author": "Mohmdev",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mohmdev/pikertattoos"
  },
  "scripts": {
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 3000",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack -p 3000",
    "dev:inspect": "cross-env NODE_OPTIONS='--inspect' next dev --turbopack -p 3000",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack -p 3000",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "format:src": "prettier --write src --config prettier.config.js",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "generate:importmap": "payload generate:importmap",
    "generate:schema": "payload-graphql generate:schema",
    "generate:types": "payload generate:types",
    "migrate": "payload migrate",
    "migrate:status": "payload migrate:status",
    "migrate:down": "payload migrate:down",
    "migrate:reset": "payload migrate:reset",
    "migrate:fresh": "payload migrate:fresh",
    "migrate:create": "payload migrate:create",
    "ci": "NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install --frozen-lockfile",
    "seed:all": "payload run src/app/next/seedScript.ts",
    "update-deps": "rm node_modules -R -Force && pnpm i"
  },
  "dependencies": {
    "@geist-ui/icons": "^1.0.2",
    "@neondatabase/serverless": "^0.10.4",
    "@payloadcms/db-postgres": "latest",
    "@payloadcms/email-resend": "latest",
    "@payloadcms/live-preview-react": "^3.12.0",
    "@payloadcms/next": "latest",
    "@payloadcms/plugin-form-builder": "latest",
    "@payloadcms/plugin-nested-docs": "latest",
    "@payloadcms/plugin-redirects": "latest",
    "@payloadcms/plugin-search": "latest",
    "@payloadcms/plugin-seo": "latest",
    "@payloadcms/richtext-lexical": "latest",
    "@payloadcms/storage-s3": "latest",
    "@payloadcms/ui": "latest",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-aspect-ratio": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-menubar": "^1.1.4",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toggle": "^1.1.1",
    "@tanstack/react-query": "^5.62.10",
    "@types/ejs": "^3.1.5",
    "@vercel/postgres": "^0.10.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "ejs": "^3.1.10",
    "embla-carousel-react": "^8.5.1",
    "graphql": "^16.10.0",
    "juice": "^11.0.0",
    "lucide-react": "^0.469.0",
    "motion": "^11.15.0",
    "next": "^15.1.3",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.4",
    "path": "^0.12.7",
    "payload": "latest",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "sass": "^1.83.0",
    "sharp": "^0.33.5",
    "sonner": "^1.7.1",
    "swiper": "^11.1.15",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.17.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "typescript-eslint": "^8.18.2"
  },
  "engines": {
    "node": ">=18.20.2 <23.0.0",
    "pnpm": ">=9"
  }
}
