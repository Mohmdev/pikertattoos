{
  "name": "pikertattoos",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "author": "Mohmdev",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohmdev/nexweb-base-template"
  },
  "scripts": {
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack -p 3000",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack -p 3000",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "format:src": "prettier --write src --config prettier.config.js",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "generate:importmap": "payload generate:importmap",
    "generate:schema": "payload-graphql generate:schema",
    "generate:types": "payload generate:types",
    "migrate": "payload migrate",
    "migrate:status": "payload migrate:status",
    "migrate:down": "payload migrate:down",
    "migrate:reset": "payload migrate:reset",
    "migrate:fresh": "payload migrate:fresh",
    "migrate:create": "payload migrate:create"
  },
  "dependencies": {
    "next": "15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.17.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "typescript-eslint": "^8.18.2"
  }
}
